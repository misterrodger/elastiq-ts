{
  "name": "elastiq",
  "version": "0.1.0-beta",
  "description": "Type-safe, lightweight Elasticsearch query builder with fluent API",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "audit": "better-npm-audit audit --level=high",
    "prepublishOnly": "npm run build && npm test && npm run lint",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misterrodger/elastiq.git"
  },
  "author": "misterrodger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misterrodger/elastiq/issues"
  },
  "homepage": "https://github.com/misterrodger/elastiq#readme",
  "keywords": [
    "elasticsearch",
    "query-builder",
    "typescript",
    "type-safe",
    "typesafe",
    "elastic",
    "search",
    "query-dsl",
    "query-builder",
    "fluent-api",
    "type-safety",
    "strongly-typed",
    "autocomplete",
    "elasticsearch-client",
    "query-dsl",
    "chain",
    "chainable"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "better-npm-audit": "^3.11.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
